  "description": "A shim for using PHP >=5.5's DateTimeInterface in a PHP >=5.3 application",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Jonathan Eskew",
      "email": "jonathan@jeskew.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DateTimeCompat\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.3.0",
    "nesbot/Carbon": "^1.21"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  }
